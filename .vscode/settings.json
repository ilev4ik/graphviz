{
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "googletest.failed",
                "settings": {
                    "foreground": "#f00"
                }
            },
            {
                "scope": "googletest.passed",
                "settings": {
                    "foreground": "#0f0"
                }
            },
            {
                "scope": "googletest.run",
                "settings": {
                    "foreground": "#0f0"
                }
            }
        ]
    },
    "C_Cpp.intelliSenseEngineFallback": "Enabled",
    "gtest-adapter.debugConfig": "Tests",
    "gtest-adapter.supportLocation": true,
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
    "clang-format.executable": "C:/Program Files (x86)/LLVM/bin/clang-format.exe",
    "clang-format.assumeFilename": ".clang-format",
    "licenser.author": "Levon Oganesyan",
    "licenser.projectName": "Graphviz",
    "licenser.license": "MIT",
    "licenser.disableAutoHeaderInsertion": true,
    "C_Cpp.configurationWarnings": "Disabled"
}